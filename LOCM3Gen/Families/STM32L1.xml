<?xml version="1.0" encoding="utf-8"?>
<!-- Microcontroller family definition -->
<family name="STM32L1" core="cortex-m3" fp-type="soft">
  <!-- Family-specific libopencm3 source files -->
  <sources>
    <include project-directory="libopencm3/cm3">
      include/libopencm3/cm3/*.h
    </include>
    <include project-directory="libopencm3/dispatch">
      include/libopencm3/dispatch/*.h
    </include>
    <include project-directory="libopencm3/usb">
      include/libopencm3/usb/*.h
    </include>
    <include project-directory="libopencm3/usb/dwc">
      include/libopencm3/usb/dwc/*.h
    </include>
    <include project-directory="libopencm3/stm32">
      include/libopencm3/stm32/*.h
    </include>
    <include project-directory="libopencm3/stm32/common">
      include/libopencm3/stm32/common/*.h
    </include>
    <include project-directory="libopencm3/stm32/l1">
      include/libopencm3/stm32/l1/*.h
    </include>
    <include project-directory="libopencm3">
      lib/cortex-m-generic.ld
    </include>
    <include project-directory="libopencm3">
      lib/libopencm3_stm32l1.a
    </include>
  </sources>

  <!-- Family-specific templates used for project generation -->
  <templates>
    <include project-directory="project" keep-existing="true">
      project/*
    </include>
    <include project-directory=".">
      linker.ld
    </include>
  </templates>
  
  <!-- Precompiled .a libraries (after being copied) -->
  <libraries>
    <library>
      libopencm3/libopencm3_stm32l1.a
    </library>
  </libraries>
  
  <!-- Compiler definitions -->
  <definitions>
    <define>
      STM32L1
    </define>
  </definitions>

  <!-- SVD files archive -->
  <svd-archive project-directory=".">
    SVD/STM32L1.zip
  </svd-archive>

