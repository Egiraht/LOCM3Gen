    <device name="STM32F207IF" rom="768K"  ram="128K" svd-file="STM32F20x.svd" />
    <device name="STM32F207IG" rom="1024K" ram="128K" svd-file="STM32F20x.svd" />
    <device name="STM32F207VC" rom="256K"  ram="128K" svd-file="STM32F20x.svd" />
