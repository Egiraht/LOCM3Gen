    <var name="SVDFile" value="STM32F0x8.svd" />
  </if-eq>

  <!-- Unzipping SVD file. -->
  <if-neq a="{$SVDFile$}" b="">
    <unzip archive="{$ProgramDir$}/SVD/{$FamilyName$}.zip" entry="{$SVDFile$}" target-dir="{$ProjectDir$}" />
  </if-neq>

  <!-- Common templates. -->
  <copy source-dir="{$TemplatesDir$}"          target-dir="{$ProjectDir$}"         file-pattern="linker.ld"  parse="true" />
  <copy source-dir="{$TemplatesDir$}/project"  target-dir="{$ProjectDir$}/project" file-pattern="*"          parse="true" keep-existing="true" recursive="true" />
</sourcegen-script>
