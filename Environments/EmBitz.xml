<sourcegen-script version="1.0">
  <!-- Building project files list. -->
  <list name="ProjectFiles">
    <add-files source="{$ProjectDir$}/*.h" relative="true" recursive="true" />
    <add-files source="{$ProjectDir$}/*.c" relative="true" recursive="true" />
  </list>
  
  <!-- Environment-specific templates used for project generation. -->
  <copy source="{$TemplatesDir$}/EmBitz/*" destination="{$ProjectDir$}" parse="true" recursive="true" />
</sourcegen-script>
